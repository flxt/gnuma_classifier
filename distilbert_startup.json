{
	"id": "distilbert",
	"address": "tba",
	"hyperParameters": 
	[
		{
			"name": "model_name",
			"type": "STRING",
			"value_list": null,
			"optional": false,
			"default": null,
			"description": "A short expressive model name.",
		},
		{
			"name": "data_location",
			"type": "STRING",
			"value_list": null,
			"optional": false,
			"default": null,
			"description": "The remote location where the training data can be loaded.",
		},
		{
			"name": "best_model",
			"type": "BOOLEAN",
			"value_list": null,
			"optional": true,
			"default": true,
			"description": "States if the best model should be loaded at the end.",
		},
		{
			"name": "steps",
			"type": "INTEGER",
			"value_list": null,
			"optional": true,
			"default": 500,
			"description": "After this number of steps a checkpoint is made.",
		},
		{
			"name": "batch_size",
			"type": "INTEGER",
			"value_list": null,
			"optional": true,
			"default": 16,
			"description": "The batch size for training and evaluation",
		},
		{
			"name": "learning_rate",
			"type": "DOUBLE",
			"value_list": null,
			"optional": true,
			"default": 2e-5,
			"description": "The learning rate during training.",
		},
		{
			"name": "epochs",
			"type": "DOUBLE",
			"value_list": null,
			"optional": true,
			"default": 3.0,
			"description": "The number of training epochs.",
		},
		{
			"name": "warmup_ratio",
			"type": "DOUBLE",
			"value_list": null,
			"optional": true,
			"default": 0.0,
			"description": "The ratio of training steps used for warmup.",
		},
		{
			"name": "adam_weigth_decay",
			"type": "DOUBLE",
			"value_list": null,
			"optional": true,
			"default": 0.0,
			"description": "Weight decay for AdamW optimizer.",
		},
		{
			"name": "adam_beta1",
			"type": "DOUBLE",
			"value_list": null,
			"optional": true,
			"default": 0.9,
			"description": "Beta1 for AdamW optimizer.",
		},
		{
			"name": "adam_beta2",
			"type": "DOUBLE",
			"value_list": null,
			"optional": true,
			"default": 0.999,
			"description": "Beta2 for AdamW optimizer. Setting this to 0.998 is recommended for large batch sizes.",
		},
				{
			"name": "adam_epsilon",
			"type": "DOUBLE",
			"value_list": null,
			"optional": true,
			"default": 1e-8,
			"description": "Epsilon for AdamW optimizer.",
		}
	]
}
		